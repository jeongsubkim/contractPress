<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="react.cap.mapper.ListMapper">
    <select id="getTable" parameterType="react.cap.bean.MTable" resultType="react.cap.bean.MTable">
    SELECT
        *
    FROM
        M_Table
    LIMIT
        100
    </select>
</mapper>